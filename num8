#include<bits/stdc++.h>
using namespace std;
int main()
{
    int tt,cnt=1;
    cin>>tt;

    while(tt--)
    {
        vector<int>vec(3);

        for(int i=0;i<3;i++)cin>>vec[i];

        sort(vec.begin(),vec.end());

        cout<<"Case "<<cnt<<": ";

        for(auto &i:vec)cout<<i<<" ";

        cout<<endl;

        cnt++;


    }
    
    return 0;
}
