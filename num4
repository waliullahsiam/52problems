#include<bits/stdc++.h>
using namespace std;
int main()
{
   int tt,flag=1;
   cin>>tt;

   while(tt--)
   {
       int n;
       cin>>n;
       cout<<"Case "<<flag<<": ";

       for(int i=1;i<=n;i++)
       {
           if(n%i==0)
           {
               cout<<i<<" ";
           }

       }
       flag++;

       cout<<endl;
   }
   
   return 0;


}
